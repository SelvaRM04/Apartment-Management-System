<head>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<%= form_with(url: add_house_path, method: :post) do |form| %>
  
  


  <div>
    <%= form.label :apartment, style: "display: block" %>
    <%= form.text_field :apartment, :required => true %>
  </div>

  <div>
    <%= form.label :block, style: "display: block" %>
    <%= form.text_field :block %>
  </div>

  <div>
    <%= form.label :doorno, style: "display: block" %>
    <%= form.text_field :doorno %>
  </div>

  <div>
  <%= button_to "Add", add_house_path, method: :post, class:"btn btn-success" %>
  </div>

<% end %>
